<!DOCTYPE grupo[
    <!ELEMENT nombre (#PCDATA)>
    <!ELEMENT tutor (#PCDATA)>
    <!ELEMENT estudiante (nombre, tutor?)>
    <!ELEMENT grupo (estudiante)>
    <!ATTLIST estudiante ID CDATA "1">
    <!ATTLIST tutor IDREF CDATA "1">
]>

<grupo>
    <estudiante ID="1">
        <nombre></nombre>
        <tutor IDREF="1"></tutor>
    </estudiante>
</grupo>
