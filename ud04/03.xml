<!DOCTYPE tabla[
    <!ELEMENT descripcion (#PCDATA)>
    <!ELEMENT dia (#PCDATA)>
    <!ELEMENT mes (#PCDATA)>
    <!ELEMENT anyo (#PCDATA)>
    <!ELEMENT fecha (dia, mes, anyo)>
    <!ELEMENT row (descripcion, fecha)>
    <!ELEMENT tabla (row+)>
    <!ATTLIST tabla
        xmlns:xsi CDATA #IMPLIED
        xsi:noNamespaceSchemaLocation CDATA #IMPLIED>
]>

<tabla xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='Schema3.xsd'>
    <row>
        <descripcion>Se funda YouTube</descripcion>
        <fecha>
             <dia>15</dia>
            <mes>2</mes>
            <anyo>2005</anyo>
        </fecha>
    </row>
    <row>
        <descripcion>Se funda Google</descripcion>
        <fecha>    
            <dia>4</dia>
            <mes>9</mes>
            <anyo>1998</anyo>
        </fecha>
    </row>
    <row>
        <descripcion>Se funda Facebook</descripcion>
        <fecha>
            <dia>4</dia>
            <mes>2</mes>
            <anyo>2004</anyo>
        </fecha>
    </row>
</tabla>
